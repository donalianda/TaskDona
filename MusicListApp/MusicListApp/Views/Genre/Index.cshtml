@model IEnumerable<MusicListApp.MST_GENRE>

@{
    ViewBag.Title = "Index Genre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index Genre</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Genre ID
        </th>
        <th>
            Genre
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GenreID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {id=item.GenreID /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Details", "Details", new {id=item.GenreID /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new {id=item.GenreID /* id=item.PrimaryKey */ })
        </td>
    </tr>
}

</table>
